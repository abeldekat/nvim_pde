{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "codeLens": { "enable": true },
  "completion": { "callSnippet": "Replace" },
  // "diagnostics": { "disable": ["unused-local"] },
  "doc": { "privateName": ["^_"] },
  "hint": {
    "enable": true,
    "setType": false,
    "paramType": true,
    "paramName": "Disable",
    "semicolon": "Disable",
    "arrayIndex": "Disable"
  },
  "runtime": {
    "path": ["?.lua", "?/init.lua"],
    "pathStrict": true,
    "version": "LuaJIT"
  },
  "workspace": {
    "checkThirdParty": false,
    "ignoreDir": ["lua"],
